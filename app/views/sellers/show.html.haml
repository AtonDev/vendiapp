.container
	.row
		.col-md-4.col-md-offset-4
			%h2.text-center
				Hello
				=@seller.full_name.split()[0]
			%dl.dl-horizontal
				%dt Name:
				%dd 
					=@seller.full_name
			%dl.dl-horizontal
				%dt Phone Number:
				%dd 
					=@seller.phone_number 
			%dl.dl-horizontal
				%dt Email:
				%dd 
					=@seller.email
			=link_to "edit", edit_seller_registration_path, :class => "pull-right"